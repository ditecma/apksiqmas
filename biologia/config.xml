<?xml version='1.0' encoding='utf-8'?>
<widget id="con.biologia.biologia" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Biolog√≠a</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon src="www/img/icon.png" density="ldpi" />
        <icon src="www/img/icon.png" density="mdpi" />
        <icon src="www/img/icon.png" density="hdpi" />
        <icon src="www/img/icon.png" density="xhdpi" />
        <icon src="www/img/icon.png" density="xxhdpi" />
        <icon src="www/img/icon.png" density="xxxhdpi" />
        <splash src="www/img/icon.png" density="land-hdpi"/>
        <splash src="www/img/icon.png" density="land-ldpi"/>
        <splash src="www/img/icon.png" density="land-mdpi"/>
        <splash src="www/img/icon.png" density="land-xhdpi"/>
        <splash src="www/img/icon.png" density="land-xhdpi"/>
        <splash src="www/img/icon.png" density="land-xhdpi"/>
        <splash src="www/img/icon.png" density="port-hdpi"/>
        <splash src="www/img/icon.png" density="port-ldpi"/>
        <splash src="www/img/icon.png" density="port-mdpi"/>
        <splash src="www/img/icon.png" density="port-xhdpi"/>
        <splash src="www/img/icon.png" density="port-xxhdpi"/>
        <splash src="www/img/icon.png" density="port-xxxhdpi"/>
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
